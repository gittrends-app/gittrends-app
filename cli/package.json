{
  "name": "@gittrends-app/cli",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "lint": "eslint --ext .ts ./src",
    "compile": "tsc --build",
    "build:clean": "shx rm -rf ./dist tsconfig.tsbuildinfo",
    "build": "run-s build:clean compile",
    "tsx": "tsx -r \"dotenv-flow/config\"",
    "migrate:up": "tsx -r \"dotenv-flow/config\" src/knex/migrate.ts up",
    "migrate:down": "tsx -r \"dotenv-flow/config\" src/knex/migrate.ts down",
    "migrate:latest": "tsx -r \"dotenv-flow/config\" src/knex/migrate.ts latest",
    "dev": "tsx  -r \"dotenv-flow/config\" src/scripts/main.ts",
    "start": "node -r \"dotenv-flow/config\" dist/scripts/main.js",
    "jest": "jest --runInBand",
    "test": "jest --runInBand",
    "test:coverage": "jest --collect-coverage",
    "verify": "run-p lint build test",
    "prettier": "prettier --write src"
  },
  "dependencies": {
    "@bull-board/api": "^5.21.1",
    "@bull-board/express": "^5.21.1",
    "@inquirer/prompts": "^5.1.3",
    "@resolid/cache-manager-sqlite": "^5.1.6",
    "better-sqlite3": "^11.2.1",
    "bullmq": "^5.12.0",
    "cache-manager": "^5.7.6",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "consola": "^3.2.3",
    "dayjs": "^1.11.12",
    "express": "^4.19.2",
    "fetch-retry": "^6.0.0",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.8.0",
    "object-hash": "^3.0.0",
    "p-limit": "^6.1.0",
    "p-queue": "^8.0.1",
    "pluralize": "^8.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.6",
    "@types/object-hash": "^3.0.6",
    "@types/pluralize": "^0.0.33"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
