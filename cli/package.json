    "better-sqlite3": "^11.7.0",
    "bullmq": "^5.34.5",
    "cache-manager": "^5.7.6",
    "chalk": "^5.4.1",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "consola": "^3.3.3",
    "dayjs": "^1.11.13",
    "express": "^4.21.2",
    "fetch-retry": "^6.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.12.0",
    "object-hash": "^3.0.0",
