{
  "extends": "@tsconfig/node20/tsconfig.json",
  "compilerOptions": {
    "target": "es2016",
    "module": "NodeNext",
    "declaration": true,
    "declarationMap": true,
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
