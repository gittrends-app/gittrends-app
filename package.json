  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "lint": "eslint src --ext .ts",
    "prepare": "husky",
    "build": "tsc",
